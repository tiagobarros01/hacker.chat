{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-continue": "off",
        "radix": "off",
        "guard-for-in": "off",
        "no-param-reassign": "off"
    }
}
